{
  "name": "@basketry/smithy",
  "version": "0.0.0",
  "description": "Basketry parser for Smithy service definitions",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "clean": "run-s -s clean:*",
    "create-snapshot": "ts-node ./src/snapshot/create-snapshot.ts",
    "lint": "run-s -s lint:*",
    "fix": "run-s -s fix:*",
    "clean:output": "rimraf lib",
    "start": "node ./lib/index.js",
    "prebuild": "run-s -s clean lint",
    "build": "run-s -s build:*",
    "build:grammar": "./node_modules/apg-js/bin/apg.sh -i ./src/ast/smithy.abnf -o ./src/ast/smithy.js",
    "build:types": "ts-node '/Users/stephenkon/code/basketry/smithy/src/ast/build-types.ts' > ./src/ast/types.ts",
    "build:syntax": "ts-node '/Users/stephenkon/code/basketry/smithy/src/ast/build-syntax.ts' > ./src/ast/stubs.ts",
    "build:typescript": "tsc",
    "lint:eslint": "eslint src/**/*.*",
    "fix:eslint": "eslint --fix src/**/*.*",
    "lint:prettier": "prettier -c .",
    "fix:prettier": "prettier -w .",
    "clean:coverage": "rimraf coverage",
    "pretest": "run-s -s clean",
    "prepack": "run-s -s build"
  },
  "keywords": [],
  "author": "Steve Konves",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basketry/smithy.git"
  },
  "bugs": {
    "url": "https://github.com/basketry/smithy/issues"
  },
  "homepage": "https://github.com/basketry/smithy#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/json-to-ast": "^2.1.2",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "apg-js": "^4.4.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "basketry": "^0.1.1",
    "case": "^1.6.3"
  }
}
